{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "CorsConfig": {
    "Policies": [
      {
        "Name": "CorsPolicy",
        "AllowedOrigins": [ "*" ],
        "AllowedMethods": [ "GET", "PUT", "DELETE", "POST" ],
        "AllowedHeader": [ "accept" ],
        "AllowWildcardSubdomains": false
      }
    ]
  }
}
